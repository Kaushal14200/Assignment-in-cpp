#include<bits/stdc++.h>
using namespace std;
int main() {
    int num1,num2,num3;
    cin>>num1>>num2>>num3;
    if (num1>num2 && num1>num3)
    {
        cout<<num1<<" : "<<"is the largest number";
    }
    if (num2>num3 && num2>num1)
    {
        cout<<num2<<" : "<<"is the largest number";
    }
    if (num3>num1 && num3>num2)
    {
        cout<<num3<<" : "<<"is the largest number";
    }
    return 0;
    
}
